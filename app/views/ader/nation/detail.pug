extends ../aalayout/layout

block content
	.container.mt-3
		nav(aria-label="breadcrumb")
			ol.breadcrumb
				li.breadcrumb-item: a(href="/ader") Home
				li.breadcrumb-item: a(href="/adNations") 国家列表
				li.breadcrumb-item.active(aria-current="page") 国家信息
		.row
			.col-12.text-right
				include ./detail/upInfo
				span &nbsp;
				include ./detail/upCode
		include ./detail/thisInfo
	input#extent(type="hidden", value=Stint.extent.nation)
	script(src="/ader/platBasicEdit.js")